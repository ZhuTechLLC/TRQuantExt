LABEL version="2.0"

# 设置环境变量
ENV PYTHONUNBUFFERED=1 \
    PYTHONDONTWRITEBYTECODE=1 \
    PIP_NO_CACHE_DIR=1 \
    PIP_DISABLE_PIP_VERSION_CHECK=1 \
    DEBIAN_FRONTEND=noninteractive \
    QT_X11_NO_MITSHM=1 \
    QT_QPA_PLATFORM=xcb

WORKDIR /app

# 安装系统依赖（包括GUI支持）
RUN apt-get update && apt-get install -y --no-install-recommends \
    # 编译工具
    gcc g++ make wget \
    # X11和Qt依赖
    libx11-6 libxcb1 libxkbcommon0 libxkbcommon-x11-0 \
    libxcb-icccm4 libxcb-image0 libxcb-keysyms1 libxcb-randr0 \
    libxcb-render-util0 libxcb-shape0 libxcb-xfixes0 libxcb-xinerama0 \
    libxcb-cursor0 libxcb-util1 \
    libegl1 libgl1-mesa-glx libglib2.0-0 libdbus-1-3 \
    libfontconfig1 libfreetype6 \
    # 字体
    fonts-noto-cjk fonts-wqy-microhei \
    # 工具
    xdg-utils git curl \
    && rm -rf /var/lib/apt/lists/*

# 安装TA-Lib
RUN wget -q http://prdownloads.sourceforge.net/ta-lib/ta-lib-0.4.0-src.tar.gz && \
    tar -xzf ta-lib-0.4.0-src.tar.gz && \
    cd ta-lib/ && \
    ./configure --prefix=/usr && \
    make -j$(nproc) && \
    make install && \
    cd .. && \
    rm -rf ta-lib ta-lib-0.4.0-src.tar.gz

# 复制requirements并安装Python依赖
COPY requirements.txt .
RUN pip install --upgrade pip && \
    pip install -r requirements.txt

# 复制项目文件
COPY . .

# 创建必要目录
RUN mkdir -p data logs results config strategies/ptrade strategies/qmt strategies/quantconnect \
    data/ptrade/backtest_results data/ptrade/trade_records \
    data/qmt/backtest_results data/qmt/trade_records \
    data/quantconnect/backtest_results data/quantconnect/trade_records

# 设置权限
RUN chmod +x *.py *.sh scripts/*.py 2>/dev/null || true

# 暴露端口
# 5000: Dashboard (Flask)
# 8001: PTrade Bridge
# 8002: QMT Bridge  
# 8003: QuantConnect Bridge
EXPOSE 5000 8001 8002 8003

# 默认启动GUI
CMD ["python3", "JQQuant.py"]

